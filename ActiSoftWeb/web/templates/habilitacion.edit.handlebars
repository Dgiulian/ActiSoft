<form action="HabilitacionEdit" method="POST" enctype="multipart/form-data" name="upload_form" id="upload_form">
  <div class="row">
    <div class="col-md-12">
      <form class="form-vertical">
        <input id="id" name="id" type="hidden" class="" value="{{id}}" >
          <input id="id_responsable" name="id_responsable" type="hidden" class="" value="{{id_responsable}}" >
          <input id="id_proveedor" name="id_proveedor" type="hidden" class="" value="{{id_proveedor}}" >
         <div class="form-group">
            <label class="col-md-4 control-label" for="fecha">Fecha:</label>
            <div class="col-md-8">
            <input id="fecha" name="fecha" type="text" class="form-control input-md date-picker" value="{{fecha}}">
         </div>
         <div class="form-group">
            <label class="col-md-4 control-label" for="nombre">Archivo:</label>
            <div class="col-md-8">
            <input id="archivo" name="archivo" type="file" class="" value="">
            <div id="progress">
                  <div id="bar"></div>
                  <div id="percent">0%</div >
              </div>
            <div id="message"></div>
         </div>
      </div>
      </form>
    </div>
  </div>
</form>